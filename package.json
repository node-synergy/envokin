{
  "name": "envokin",
  "version": "1.0.4",
  "description": "Envokin is a powerful and intuitive environment variable validation library for Node.js applications. Designed to ensure the integrity and correctness of your environment configurations, Envokin simplifies the process of validating environment variables, making your application more robust and secure.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts'",
    "test": "tsx --test \"tests/**/*.spec.ts\"",
    "build": "rimraf lib && tsc --build tsconfig.build.json",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    ".env",
    "envokin",
    "validation",
    "environment"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/node-synergy/envokin"
  },
  "engines": {
    "node": ">=20.15.0"
  },
  "type": "module",
  "author": "Elkost",
  "license": "MIT",
  "imports": {
    "#lib/*": "./lib/*",
    "#core/*": "./lib/core/*",
    "#types/*": "./lib/types/*"
  },
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@stylistic/eslint-plugin-js": "^2.3.0",
    "@stylistic/eslint-plugin-ts": "^2.3.0",
    "@types/node": "^20.14.9",
    "dotenv": "^16.4.5",
    "eslint": "^9.6.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "rimraf": "^5.0.7",
    "tsx": "^4.16.2",
    "type-fest": "^4.21.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "8.0.0-alpha.39"
  }
}
